<section class="sub section-{{ section.data.pageClass }}">
	<div class="section-head">
		<h2 class="shaded">{{ section.data.title | safe }}</h2>
	</div>
	{% if section.data.img %}
	<img src="/_assets/img/{{ section.data.img }}"{% if section.data.fullbleed %} class="fullbleed"{% endif %} alt="{{ section.data.alt}}">
	{% endif %}
	<div class="copy">
		{{ section.data.content | safe }}
	</div>
</section>